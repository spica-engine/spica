{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://spica.internal/passport/forgot-password-verify",
  "type": "object",
  "title": "Forgot password verify schema",
  "required": ["username", "strategy", "code", "newPassword"],
  "properties": {
    "username": {
      "type": "string",
      "title": "Username"
    },
    "code": {
      "type": "string",
      "title": "Verification code",
      "minLength": 1
    },
    "strategy": {
      "type": "string",
      "title": "Strategy"
    },
    "newPassword": {
      "type": "string",
      "title": "New password",
      "minLength": 3
    }
  },
  "additionalProperties": false
}
