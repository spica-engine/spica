{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://spica.internal/passport/apikey",
  "type": "object",
  "required": ["name"],
  "properties": {
    "_id": {
      "type": "string",
      "format": "objectid",
      "description": "Unique objectid of the apikey"
    },
    "key": {
      "type": "string",
      "description": "Key of the apikey."
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "active": {
      "type": "boolean",
      "default": true
    }
  },
  "additionalProperties": false
}
