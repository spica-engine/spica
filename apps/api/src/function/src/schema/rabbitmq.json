{
  "$id": "http://spica.internal/function/enqueuer/rabbitmq",
  "title": "RabbitMQ",
  "description": "RabbitMQ enqueuer for functions",
  "type": "object",
  "required": ["url"],
  "properties": {
    "url": {
      "title": "URl",
      "type": "string"
    },
    "exchange": {
      "title": "Exchange",
      "type": "object",
      "required": ["name", "type"],
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "type": {
          "title": "Type",
          "type": "string",
          "enum": ["direct", "fanout", "topic", "headers"]
        },
        "durable": {
          "title": "Durable",
          "type": "boolean",
          "default": false
        },
        "pattern": {
          "title": "Pattern",
          "type": "string",
          "default": ""
        },
        "headers": {
          "title": "Headers",
          "type": "object",
          "additionalProperties": {
            "type": ["string", "number", "boolean"]
          }
        }
      }
    },
    "queue": {
      "title": "Queue",
      "type": "object",
      "default": {
        "name": "",
        "durable": false
      },
      "properties": {
        "name": {
          "title": "Name",
          "type": "string",
          "default": ""
        },
        "durable": {
          "title": "Durable",
          "type": "boolean",
          "default": false
        },
        "exclusive": {
          "title": "Exclusive",
          "type": "boolean"
        }
      }
    },
    "prefetch": {
      "title": "Prefetch",
      "type": "integer"
    },
    "noAck": {
      "title": "No Acknowledgement",
      "type": "boolean",
      "default": true
    }
  },
  "additionalProperties": false
}
